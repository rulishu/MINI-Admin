[
  {
    "path": "/login",
    "component": "@/layouts/UserLayout"
  },
  {
    "path": "/",
    "component": "@/layouts/BasicLayout",
    "routes": [
      {
        "path": "/",
        "redirectTo": "/welcome"
      },
      {
        "path": "/welcome",
        "name": "首页",
        "icon": "welcome",
        "component": "@/pages/home"
      },
      {
        "path": "/cust",
        "name": "客户",
        "icon": "user",
        "routes": [
          {
            "path": "/cust/custManage",
            "name": "客户管理",
            "routes": [
              {
                "path": "/cust/custManage/memberManage",
                "name": "会员管理",
                "component": "@/pages/cust/memberManage"
              },
              {
                "path": "/cust/custManage/tagsManage",
                "name": "标签管理",
                "component": "@/pages/cust/tagsManage"
              }
            ]
          },
          {
            "path": "/cust/memberSettings",
            "name": "会员设置",
            "routes": [
              {
                "path": "/cust/memberSettings/equityRules",
                "name": "权益规则",
                "component": "@/pages/cust/memberSettings/EquityRules"
              }
            ]
          }
        ]
      }
    ]
  }
]
