        "name": "首页",
        "icon": "welcome",
        "component": "@/pages/home"
      },
      {
        "path": "/product",
        "name": "商品",
        "icon": "dashboard",
        "routes": [
          {
            "path": "/product/productManage",
            "name": "商品管理",
            "component": "@/pages/product/productManage"
