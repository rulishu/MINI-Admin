{
  "name": "admin",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "max dev",
    "build": "max build",
    "umiinstall": "max setup",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint:js": "cross-env TIMING=1 eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" ",
    "prettier": "prettier -c --write \"**/*\""
  },
  "lint-staged": {
    "**/*.less": "stylelint",
    "**/*.{js,jsx,ts,tsx}": "npm run lint:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@antdp/authorized": "2.0.4",
    "@antdp/basic-layouts": "2.0.4",
    "@antdp/hooks": "2.0.4",
    "@antdp/user-login": "2.0.4",
    "@antdp/antdp-ui":"2.0.4",
    "@antdp/request": "2.0.4",
    "antd": "5.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "overrides": {
    "dva": "^2.6.0-beta.20",
    "antd": "5.4.7"
  },
  "resolutions": {
    "dva": "^2.6.0-beta.20",
    "antd": "5.4.7"
  },
  "devDependencies": {
    "@antdp/config": "2.0.4",
    "@antdp/dependencies": "2.0.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/max": "~4.0.47",
    "@umijs/plugin-model": "~2.6.2",
    "@umijs/fabric":"~2.14.1",
    "typescript": "^5.0.0",
    "husky":"~8.0.3",
    "eslint-plugin-prettier":"~4.2.1"
  }
}
